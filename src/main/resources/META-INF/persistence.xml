<persistence version="1.0" 
    xmlns="http://java.sun.com/xml/ns/persistence" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
    <persistence-unit name="manager1" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    
    <class>model.Car</class>
    
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    
       <properties>
       <property name="javax.persistence.jdbc.url" value="jdbc:hsqldb:hsql://localhost/"/>
       <!-- <property name="javax.persistence.jdbc.url" value="jdbc:hsqldb:mem:execShareDb"/> --> 
       <property name="javax.persistence.jdbc.driver" value="org.hsqldb.jdbcDriver"/>
    <property name="javax.persistence.jdbc.user" value="sa"/>
    <property name="javax.persistence.jdbc.password" value=""/>
    <property name="eclipselink.ddl-generation" value="create-or-extend-tables"/>
<property name="eclipselink.ddl-generation.output-mode" value="database"/>
<property name="eclipselink.logging.level" value="FINE"/>
    
        </properties>
    </persistence-unit>
    
    
    
    
        <persistence-unit name="entityManagerForTesting" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    
    <class>org.oLabDynamics.model.Author</class>
    <class>org.oLabDynamics.model.Publication</class>
    <class>org.oLabDynamics.model.WorkingPaper</class>
    <class>org.oLabDynamics.model.PublishedPaper</class>
    <class>org.oLabDynamics.model.CompanionSite</class>
    <class>org.oLabDynamics.model.Code</class>
    <class>org.oLabDynamics.model.InputData</class>
    <class>org.oLabDynamics.model.Constraint</class>
    <class>org.oLabDynamics.model.ThematicSite</class>
    <class>org.oLabDynamics.model.OutputData</class>
    <class>org.oLabDynamics.model.ThematicSite</class>
    
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    
       <properties>
       <!-- <property name="javax.persistence.jdbc.url" value="jdbc:hsqldb:hsql://localhost/"/> -->
       <property name="javax.persistence.jdbc.url" value="jdbc:hsqldb:mem:execShareDb"/> 
       <property name="javax.persistence.jdbc.driver" value="org.hsqldb.jdbcDriver"/>
    <property name="javax.persistence.jdbc.user" value="sa"/>
    <property name="javax.persistence.jdbc.password" value=""/>
    <property name="eclipselink.ddl-generation" value="create-or-extend-tables"/>
<property name="eclipselink.ddl-generation.output-mode" value="database"/>
<property name="eclipselink.logging.level" value="FINE"/>
    
        </properties>
    </persistence-unit>
</persistence>

